{
  "name": "docchi",
  "description": "Documentation generating tool for JavaScript.",
  "version": "0.12.0",
  "license": "GPL-3.0",
  "author": {
    "email": "0x8890@airmail.cc",
    "url": "http://0x8890.com"
  },
  "homepage": "https://github.com/0x8890/docchi",
  "repository": {
    "type": "git",
    "url": "git@github.com:0x8890/docchi.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "eslint lib test && node test",
    "postpublish": "git tag `npm v docchi version` && git push origin --tags"
  },
  "dependencies": {
    "acorn": "^3.0.4",
    "commonmark": "^0.25.0",
    "doctrine": "^1.2.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "acorn-es7-plugin": "^1.0.12",
    "eslint": "^2.5.1",
    "eslint-config-0x8890": "^1.0.2",
    "tape": "^4.5.1"
  },
  "eslintConfig": {
    "extends": "0x8890",
    "rules": {
      "strict": 0
    }
  },
  "files": [
    "lib/",
    "bin/",
    "LICENSE"
  ],
  "bin": {
    "docchi": "bin/docchi"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "doc",
    "docs",
    "generator",
    "es6",
    "ast",
    "tool",
    "jsdoc",
    "document",
    "comment",
    "json",
    "documentation"
  ]
