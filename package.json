{
  "name": "doc-tree",
  "description": "Documentation generating tool for JavaScript that matches comments to AST nodes.",
  "version": "0.12.2",
  "license": "GPL-3.0",
  "homepage": "https://github.com/sapeien/doc-tree",
  "repository": {
    "type": "git",
    "url": "git@github.com:sapeien/doc-tree.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "eslint lib test && node test",
    "postpublish": "git tag `npm v doc-tree version` && git push origin --tags"
  },
  "dependencies": {
    "acorn": "^4.0.3",
    "commonmark": "^0.26.0",
    "doctrine": "^1.5.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "acorn-es7-plugin": "^1.1.3",
    "eslint": "^3.8.1",
    "eslint-config-boss": "^1.0.4",
    "tape": "^4.6.2"
  },
  "eslintConfig": {
    "extends": "boss",
    "rules": {
      "strict": 0
    }
  },
  "files": [
    "lib/",
    "bin/",
    "LICENSE"
  ],
  "bin": {
    "doc-tree": "bin/doc-tree"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "doc",
    "docs",
    "generator",
    "ast",
    "tool",
    "jsdoc",
    "document",
    "comment",
    "json",
    "documentation"
  ]
}
