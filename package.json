{
  "name": "docchi",
  "description": "Documentation generating tool for JavaScript.",
  "version": "0.10.0",
  "license": "GPLv3",
  "author": "Dali Zheng <d@liwa.li> (http://daliwa.li)",
  "homepage": "https://github.com/daliwali/docchi",
  "repository": {
    "type": "git",
    "url": "https://github.com/daliwali/docchi"
  },
  "main": "dist/lib/index.js",
  "scripts": {
    "test": "make lint && make && node dist/test | tap-dot",
    "prepublish": "make clean && make compile-lib"
  },
  "dependencies": {
    "acorn": "^1.2.2",
    "babel-runtime": "^5.4.7",
    "commonmark": "^0.19.0",
    "doctrine": "^0.6.4",
    "minimist": "^1.1.1",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "eslint": "^0.21.2",
    "tap-dot": "^1.0.0",
    "tape": "^4.0.0"
